# 슬랙 테스트
curl -X POST -H "User-Agent: linux bla bla" -H "Content-Type: application/json" -d " \
{\
\"servicenm\":\"알림봇\",\
\"groupid\":\"100001\",\
\"date\":\"20220512223000\",\
\"msg\":\"알림 봇 서비스 테스트\",\
\"type\":\"1\"\
}" localhost:8080/v1/slack

# 텔레그램 테스트
curl -X POST -H "User-Agent: linux bla bla" -H "Content-Type: application/json" -d " \
{\
\"servicenm\":\"알림봇\",\
\"groupid\":\"100001\",\
\"date\":\"20220512223000\",\
\"msg\":\"알림 봇 서비스 테스트\",\
\"type\":\"1\"\
}" localhost:8080/v1/telegram


# 이메일 테스트
curl -X POST -H "User-Agent: linux bla bla" -H "Content-Type: application/json" -d " \
{\
\"address\":\"cooj_sky@naver.com\",\
\"title\":\"WE-CHECK\",\
\"message\":\"We-Check 장애 발생\"\
}" localhost:8080/v1/email
